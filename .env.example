# Trading Bot Environment Variables
# Copy this file to .env and fill in your actual values

# Binance API Credentials
TRADING_BINANCE_API_KEY=your-binance-api-key-here
TRADING_BINANCE_API_SECRET=your-binance-api-secret-here

# Grok AI API Key (for Agentic AI)
GROK_API_KEY=your-grok-api-key-here

# Database Configuration (PostgreSQL)
POSTGRES_URL=jdbc:postgresql://localhost:5432/trading_bot
POSTGRES_USER=tradingbot
POSTGRES_PASSWORD=tradingbot123

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379

# Kafka Configuration
KAFKA_BOOTSTRAP_SERVERS=localhost:9092

# Agent Configuration
AGENT_ORCHESTRATOR_ENABLED=true
AGENT_ORCHESTRATOR_LOOP_INTERVAL=30000
AGENT_LLM_GROK_ENABLED=true

# Application Port
SERVER_PORT=8080
